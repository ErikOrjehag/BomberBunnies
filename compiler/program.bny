INIT:	

/* -------- START OF MAIN -------- */

MAIN:

	jump CONTROL
	CONTROL_R:

	jump BUTTON
	BUTTON_R: 

	jump TICKBOMBS
	TICKBOMBS_R:

	jump TICKEXPLOSIONS
	TICKEXPLOSIONS_R:

	jump MAIN

/* -------- END OF MAIN -------- */

/* -------- START OF TICKEXPLOSIONS -------- */

TICKEXPLOSIONS:
	jump	BOOM1
	BOOM1_R:

	jump	BOOM2
	BOOM2_R:

	jump	TICKEXPLOSIONS_R

BOOM1:
	/* Checks if timer is equal to 1 */
	load	gr0 P1EXPLOSION1ACTIVE
	sub	gr0 1
	bne	BOOM1_R

	/* Decreases explosion timer */
	load	gr0 P1EXPLOSION1TIME
	sub	gr0 1
	store	P1EXPLOSION1TIME gr0

	/* Checks if explosion should vanish */
	load 	gr0 P1EXPLOSION1TIME
	sub	gr0 0
	bne	BOOM1_R

	/* Vanish explosion */
	load	gr0 0
	store	P1EXPLOSION1ACTIVE gr0

	/* Middle explosion tile */
	load	gr2 P1EXPLOSION1POS
	load 	gr3 GRASS
	tpoint	gr2
	twrite	gr3

	/* Explosion to the right */
	/* Checks space for explosion */
	add	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1LEFT

	/* Places explosion */
	twrite	gr3

	/* Checks space for next explosion */
	add	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1LEFT

	/* Places explosion */
	twrite	gr3

	

E1LEFT:
	/* Explosion to the left */
	load	gr2 P1EXPLOSION1POS

	/* Checks space for explosion */
	sub	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1DOWN

	/* Places explosion */
	twrite	gr3

	/* Checks space for next explosion */
	sub	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1DOWN

	/* Places explosion */
	twrite	gr3
	
E1DOWN:
 	/* Explosion down */
	load	gr2 P1EXPLOSION1POS

	/* Checks space for explosion */
	add	gr2 15
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1UP

	/* Places explosion */
	twrite	gr3

	/* Checks space for next explosion */
	add	gr2 15
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	E1UP

	/* Places explosion */
	twrite	gr3

E1UP:
	/* Explosion up */
	load	gr2 P1EXPLOSION1POS
	
	/* Checks space for explosion */
	sub	gr2 15
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	BOOM1_R

	/* Places explosion */
	twrite	gr3

	/* Checks space for next explosion */
	sub	gr2 15
	tpoint	gr2
	tread	gr0
	sub 	gr0 EXPLOSION
	bne 	BOOM1_R

	/* Places explosion */
	twrite	gr3
	
	jump BOOM1_R

BOOM2:
	/* Checks if timer is equal to 1 */
