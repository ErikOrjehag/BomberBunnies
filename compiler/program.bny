	jump BOOM2_R	


/* -------- END OF TICKEXPLOSIONS -------- */

/* -------- START OF TICKBOMBS -------- */

TICKBOMBS:
	/* Checking bombs for player 1 */
	/* Checks if bomb is placed */
	load	gr0 P1BOMB1ACTIVE
	sub	gr0 1
	bne	P1BOMB2

	/* Decrease counter with 1 */
	load	gr0 P1BOMB1TIME
	sub	gr0 1
