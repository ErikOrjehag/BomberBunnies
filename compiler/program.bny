P1EXPLOSION1INIT:
	/* Deactivate bomb */
	load	gr0 0
	store	P1BOMB1ACTIVE gr0

	/* Initializes explosion */
	load	gr0 P1BOMB1POS
	store 	P1EXPLOSION1POS gr0
	load	gr0 1
	store	P1EXPLOSION1ACTIVE gr0
	load 	gr0 2
	store	P1EXPLOSION1TIME gr0

	/* Decrease bomb counter for player 1 */
	load	gr0 P1BOMBCOUNT
	sub	gr0 1
	store	P1BOMBCOUNT gr0
	
	load	gr4 P1BOMB1POS
	jump 	EXPLODE

P1EXPLOSION2INIT:
