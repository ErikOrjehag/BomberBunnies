/* -------- START OF TICKBOMBS -------- */

TICKBOMBS:
	jump TICKBOMB1
	TICKBOMB1_R:

	jump TICKBOMB2
	TICKBOMB2_R:

	jump TICKBOMBS_R

TICKBOMB1:
	/* Checks if bomb is placed */
	load	gr0 P1BOMB1ACTIVE
	sub	gr0 1
	bne	TICKBOMB1_R

	/* Decrease counter with 1 */
	load	gr0 P1BOMB1TIME
	sub	gr0 1
	store	P1BOMB1TIME gr0

	load	gr0 0
	sub	gr0 P1BOMB1TIME
	beq	EXPLODE1
	
	jump	TICKBOMB1_R

EXPLODE1:
	/* Initializes explosion */
	load	gr0 P1BOMB1POS
	store 	P1EXPLOSION1POS gr0
	load	gr0 1
	store	P1EXPLOSION1ACTIVE gr0
	load 	gr0 2
	store	P1EXPLOSION1TIME gr0

	/* Decrease bomb counter for player 1 */
	load	gr0 P1BOMBCOUNT
	sub	gr0 1
	store	P1BOMBCOUNT gr0

	load	gr2 P1BOMB1POS
	load 	gr3 EXPLOSION
	tpoint	gr2
	twrite	gr3

	/* Explosion to the right */
	/* Checks space for explosion */
	add	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 WALL
	beq 	P1LEFT

	/* Places explosion */
