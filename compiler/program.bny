/* IDENTISK */
EXPLODE: /* (POS = GR4) */
	store	MOVE gr4
	load	gr2 MOVE
	load 	gr3 EXPLOSION
	tpoint	gr2
	twrite	gr3

EXPLODERIGHT:
	/* Explosion to the right */
	/* Checks space for explosion */
	add	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 WALL
	beq 	EXPLODELEFT

	/* Places explosion */
	twrite	gr3

	/* Checks space for next explosion */
	add	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 WALL
	beq 	EXPLODELEFT

	/* Places explosion */
	twrite	gr3

EXPLODELEFT:
	/* Explosion to the left */
	store	MOVE gr4
	load	gr2 MOVE

	/* Checks space for explosion */
	sub	gr2 1
	tpoint	gr2
	tread	gr0
	sub 	gr0 WALL
	beq 	EXPLODEDOWN

	/* Places explosion */
