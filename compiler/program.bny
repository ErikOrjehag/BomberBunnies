/* -------- START OF BUTTON -------- */

BUTTON:
	btn1	BTN1
	BTN1_R:
	
	btn2	BTN2
	BTN2_R:
	
	jump	BUTTON_R

BTN1:
	/* Checks if player can place more bombs */
	load	gr0 P1BOMBCOUNT
	sub	gr0 MAXBOMBS
	beq	BTN1_R

	/* Checks if there's already a egg there */
	store	XPOS1 gr12
	store	YPOS1 gr13
	load	gr0 YPOS1
	mul	gr0 15
	add	gr0 XPOS1
	tpoint	gr0
	tread	gr1
	sub	gr1 EGG
	beq	BTN1_R

	/* Places egg */
	store 	XPOS1 gr12
	store 	YPOS1 gr13
	load 	gr3 YPOS1
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS1
	tpoint 	gr3
	twrite 	gr2
	load	gr0 1
	store	P1BOMB1ACTIVE gr0

	/* Loads bomb position and bomb timer */
	store	P1BOMB1POS gr3
	load	gr0 16
	store	P1BOMB1TIME gr0

	/* Increases bomb counter */
	load	gr0 P1BOMBCOUNT
	add	gr0 1
	store 	P1BOMBCOUNT gr0
	jump	BTN1_R

BTN2:
	/* Checks if player can place more bombs */
	load	gr0 P2BOMBCOUNT
	sub	gr0 MAXBOMBS
	beq	BTN2_R

	/* Checks if there's already a egg there */
	store	XPOS2 gr14
	store	YPOS2 gr15
	load	gr0 YPOS2
	mul	gr0 15
	add	gr0 XPOS2
	tpoint	gr0
	tread	gr1
	sub	gr1 EGG
	beq	BTN2_R

	/* Places egg */
	store 	XPOS2 gr14
	store	YPOS2 gr15
	load 	gr3 YPOS2
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS2
	tpoint 	gr3
	twrite 	gr2
	load	gr0 1
	store	P2BOMB4ACTIVE gr0

	/* Loads bomb position and bomb timer */
	store	P2BOMB4POS gr3
	load	gr0 16
	store	P2BOMB4TIME gr0

	/* Increases bomb counter */
	load	gr0 P2BOMBCOUNT
	add	gr0 1
	store 	P2BOMBCOUNT gr0
	jump	BTN2_R

