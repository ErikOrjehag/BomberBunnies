/* -------- START OF BUTTON -------- */

BUTTON:
	btn1	BTN1
	BTN1_R:
	
	btn2	BTN2
	BTN2_R:
	
	jump	BUTTON_R

BTN1:
	/* Checks if player can place more bombs */
	load	gr0 P1BOMBCOUNT
	sub	gr0 MAXBOMBS
	beq	BTN1_R

	/* Checks if there's already a egg there */
	store	XPOS1 gr12
	store	YPOS1 gr13
	load	gr0 YPOS1
	mul	gr0 15
	add	gr0 XPOS1
	tpoint	gr0
	tread	gr1
	sub	gr1 EGG
	beq	BTN1_R

	/* Checks which bomb to place */
	load	  gr0 P1BOMB1ACTIVE
	sub	  gr0 0
	beq	  P1PLACEBOMB1

	load	  gr0 P1BOMB2ACTIVE
	sub	  gr0 0
	beq	  P1PLACEBOMB2

	load	  gr0 P1BOMB3ACTIVE
	sub	  gr0 0
	beq	  P1PLACEBOMB3

	jump 	  BTN1_R

P1INCREASEBOMBCOUNTER:
	/* Increases bomb counter */
	load	gr0 P1BOMBCOUNT
	add	gr0 1
	store 	P1BOMBCOUNT gr0
	jump	BTN1_R

P1PLACEBOMB1:
	/* Places egg */
	store 	XPOS1 gr12
	store	YPOS1 gr13
	load 	gr3 YPOS1
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS1
	tpoint 	gr3
	twrite 	gr2

	/* Initialize bomb */
	load	gr0 1
	store	P1BOMB1ACTIVE gr0
	store	P1BOMB1POS gr3
	load	gr0 16
	store	P1BOMB1TIME gr0

	jump	P1INCREASEBOMBCOUNTER

P1PLACEBOMB2:
	/* Places egg */
	store 	XPOS1 gr12
	store	YPOS1 gr13
	load 	gr3 YPOS1
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS1
	tpoint 	gr3
	twrite 	gr2
	load	gr0 1
	store	P1BOMB2ACTIVE gr0

	/* Loads bomb position and bomb timer */
	store	P1BOMB2POS gr3
	load	gr0 16
	store	P1BOMB2TIME gr0

	jump	P1INCREASEBOMBCOUNTER

P1PLACEBOMB3:
	/* Places egg */
	store 	XPOS1 gr12
	store	YPOS1 gr13
	load 	gr3 YPOS1
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS1
	tpoint 	gr3
	twrite 	gr2
	load	gr0 1
	store	P1BOMB3ACTIVE gr0

	/* Loads bomb position and bomb timer */
	store	P1BOMB3POS gr3
	load	gr0 16
	store	P1BOMB3TIME gr0

	jump	P1INCREASEBOMBCOUNTER

BTN2:
	/* Checks if player can place more bombs */
	load	gr0 P2BOMBCOUNT
	sub	gr0 MAXBOMBS
	beq	BTN2_R

	/* Checks if there's already a egg there */
	store	XPOS2 gr14
	store	YPOS2 gr15
	load	gr0 YPOS2
	mul	gr0 15
	add	gr0 XPOS2
	tpoint	gr0
	tread	gr1
	sub	gr1 EGG
	beq	BTN2_R

	/* Checks which bomb to place */
	load	  gr0 P2BOMB1ACTIVE
	sub	  gr0 0
	beq	  P2PLACEBOMB1

	load	  gr0 P2BOMB2ACTIVE
	sub	  gr0 0
	beq	  P2PLACEBOMB2

	load	  gr0 P2BOMB3ACTIVE
	sub	  gr0 0
	beq	  P2PLACEBOMB3

P2INCREASEBOMBCOUNTER:
	/* Increases bomb counter */
	load	gr0 P2BOMBCOUNT
	add	gr0 1
	store 	P2BOMBCOUNT gr0
	jump	BTN2_R

P2PLACEBOMB1:
	/* Places egg */
	store 	XPOS2 gr14
	store	YPOS2 gr15
	load 	gr3 YPOS2
	load	gr2 EGG
	mul 	gr3 15
	add	gr3 XPOS2
	tpoint 	gr3
	twrite 	gr2
	load	gr0 1
	store	P2BOMB1ACTIVE gr0

	/* Loads bomb position and bomb timer */
	store	P2BOMB1POS gr3
	load	gr0 16
	store	P2BOMB1TIME gr0

	jump	P2INCREASEBOMBCOUNTER

P2PLACEBOMB2:
	/* Places egg */
